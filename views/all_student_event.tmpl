{{ template "header" . }}

	<body>
		<h1>学生イベント一覧</h1>
		<a href="/student_event/register">新規登録</a>

		<table>
			<tr>
				<th>日付</th>
				<th>予定名</th>
			</tr>
			{{ range $i, $v := .student }}
			<tr>
				<td>{{ $v.Month }} / {{ $v.Day }}</td>
				<td><a href="/student_event/{{ $v.Id }}">{{ $v.Name }}</a></td>
			</tr>
			{{ end }}
		</table>
		<a href="/student_event">戻る</a>
		<a href="/student_event/search">検索</a>
	</body>
</html>
