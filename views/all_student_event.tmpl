{{ template "header" . }}
	</head>

	<body>
		<center>
			<h1>学生イベント一覧</h1>
			<a href="/student_event/register" class="button" id="posi-logout">新規登録</a>
	
			<table border=1>
				<tr>
					<th id="std">日付</th>
					<th id="std">予定名</th>
				</tr>
				{{ range $i, $v := .student }}
				<tr>
					<td>{{ $v.Month }} / {{ $v.Day }}</td>
					<td><a href="/student_event/show/{{ $v.Id }}">{{ $v.Name }}</a></td>
				</tr>
				{{ end }}
			</table>
			<a href="/student_event" class="button" id="posi-button">戻る</a>
			<a href="/student_event/search" class="button" id="posi-button">検索</a>
		</center>
	</body>
</html>
